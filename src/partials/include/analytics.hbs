{{#if (shared 'analytics.enabled')}}
<script async defer data-hostname='{{shared 'analytics' 'hostname' (env)}}'
  data-collect-dnt='{{shared 'analytics.ignore_dnt'}}' src='https://{{
    shared 'analytics' 'domains' (when (env 'production') 'custom' 'scripts')
  }}/latest{{
    when (env 'development') (shared 'analytics.script_dev_suffix') ''
  }}.js'></script>
<noscript>
  <img src='https://{{
      shared 'analytics' 'domains' (when (env 'production') 'custom' 'queue')
    }}/noscript.gif?hostname={{
      shared 'analytics' 'hostname' (env)
    }}&ignore-dnt={{
      shared 'analytics.ignore_dnt'
    }}' alt='' referrerpolicy='no-referrer-when-downgrade' />
</noscript>
{{/if}}
